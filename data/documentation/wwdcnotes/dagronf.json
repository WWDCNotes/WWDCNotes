ntation/WWDCNotes/stevenpaulhoward":{"title":"stevenpaulhoward (4 notes)","abstract":[{"type":"text","text":"3D connoisseur"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/stevenpaulhoward","url":"\/documentation\/wwdcnotes\/stevenpaulhoward","kind":"article","role":"sampleCode","type":"topic"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC22-110367-Simplify-C++-templates-with-concepts":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC22-110367-Simplify-C++-templates-with-concepts","title":"Simplify C++ templates with concepts","abstract":[{"text":"Discover how C++20 features can take your C++ code to the next level. We’ll introduce concepts and explore how you can use it to find errors faster in your generic C++ code. We’ll also discuss the latest enhancements to the constexpr feature and show how you can leverage it to improve your app’s performance by evaluating code at compile time.","type":"text"}],"kind":"article","url":"\/documentation\/wwdcnotes\/wwdc22-110367-simplify-c++-templates-with-concepts","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes","kind":"symbol","title":"WWDC Notes","role":"collection","images":[{"identifier":"WWDCNotes.png","type":"icon"}],"type":"topic","url":"\/documentation\/wwdcnotes","abstract":[{"text":"Session notes shared by the community for the community.","type":"text"}]},"doc://WWDCNotes/documentation/WWDCNotes/derrickshowers":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/derrickshowers","type":"topic","abstract":[{"text":"No Bio on GitHub","type":"text"}],"title":"Derrick Showers (1 note)","url":"\/documentation\/wwdcnotes\/derrickshowers","kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/JohnBaer3":{"title":"John Baer (2 notes)","url":"\/documentation\/wwdcnotes\/johnbaer3","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/JohnBaer3","type":"topic","kind":"article","abstract":[{"text":"No Bio on GitHub","type":"text"}]},"doc://WWDCNotes/documentation/WWDCNotes/ronyfadel":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/ronyfadel","type":"topic","abstract":[{"text":"Former Apple engineer turned indie developer.","type":"text"}],"title":"Rony Fadel (2 notes)","url":"\/documentation\/wwdcnotes\/ronyfadel","kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/trav-ma":{"title":"Travis Ma (5 notes)","abstract":[{"type":"text","text":"Software Development Manager for IQVIA’s Mobile team in Arizona."}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/trav-ma","url":"\/documentation\/wwdcnotes\/trav-ma","kind":"article","role":"sampleCode","type":"topic"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC22-110368-Whats-new-in-SwiftDocC":{"url":"\/documentation\/wwdcnotes\/wwdc22-110368-whats-new-in-swiftdocc","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC22-110368-Whats-new-in-SwiftDocC","type":"topic","role":"sampleCode","abstract":[{"type":"text","text":"Join us for an exciting update on Swift-DocC and learn how you can write and share documentation for your own projects. We’ll explore improvements to Swift-DocC navigation and share how you can compile documentation for application targets and Objective-C code. We’ll also show you how to publish your content straight to hosting services like GitHub Pages."}],"title":"What’s new in Swift-DocC","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/srujanc":{"title":"Srujan Chitla (5 notes)","url":"\/documentation\/wwdcnotes\/srujanc","type":"topic","abstract":[{"type":"text","text":"Sr Software Engineer at IQVIA"}],"kind":"article","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/srujanc"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC23-10252-Build-programmatic-UI-with-Xcode-Previews":{"role":"sampleCode","url":"\/documentation\/wwdcnotes\/wwdc23-10252-build-programmatic-ui-with-xcode-previews","abstract":[{"type":"text","text":"Learn how you can use the #Preview macro on Xcode 15 to quickly iterate on your UI code written in SwiftUI, UIKit, or AppKit. Explore a collage of unique workflows for interacting with views right in the canvas, find out how to view multiple variations of UI simultaneously, and discover how you can travel through your widget’s timeline in seconds to test the transitions between entries. We’ll also show you how to add previews to libraries, provide sample assets, and preview your views in your physical devices to leverage their capabilities and existing data."}],"title":"Build programmatic UI with Xcode Previews","kind":"article","type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC23-10252-Build-programmatic-UI-with-Xcode-Previews"},"doc://WWDCNotes/documentation/WWDCNotes/petermolnar-dev":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/petermolnar-dev","type":"topic","abstract":[{"type":"text","text":"iOS Developer @Parallels"}],"title":"Peter Molnar (1 note)","url":"\/documentation\/wwdcnotes\/petermolnar-dev","kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/tianskylan":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/tianskylan","type":"topic","abstract":[{"text":"No Bio on GitHub","type":"text"}],"title":"Sky Lan (2 notes)","url":"\/documentation\/wwdcnotes\/tianskylan","kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/rogerluan":{"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/rogerluan","title":"Roger Oba (6 notes)","type":"topic","abstract":[{"type":"text","text":"Core contributor @fastlane 🚀"}],"url":"\/documentation\/wwdcnotes\/rogerluan","role":"sampleCode"}}}