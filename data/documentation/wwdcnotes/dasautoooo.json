ract":[{"type":"text","text":"No Bio on GitHub"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/dagronf"},"doc://WWDCNotes/documentation/WWDCNotes/kabirkhaan":{"role":"sampleCode","type":"topic","kind":"article","url":"\/documentation\/wwdcnotes\/kabirkhaan","title":"Kabir Khan (1 note)","abstract":[{"type":"text","text":"No Bio on GitHub"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/kabirkhaan"},"doc://WWDCNotes/documentation/WWDCNotes/javierdemartin":{"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/javierdemartin","abstract":[{"type":"text","text":"I do things with iOS"}],"role":"sampleCode","type":"topic","title":"Javier de Martín (1 note)","url":"\/documentation\/wwdcnotes\/javierdemartin"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10097-Advances-in-UICollectionView":{"kind":"article","title":"Advances in UICollectionView","url":"\/documentation\/wwdcnotes\/wwdc20-10097-advances-in-uicollectionview","abstract":[{"text":"Learn about new features of UICollectionView that make it easier to use and unlock powerful new functionality. We’ll show you how to use section snapshots with your diffable data source to create outlines that can expand and collapse, and introduce you to building lists with compositional layout to create UITableView-like interfaces with a collection view. And discover modern techniques for dequeuing cells and configuring their content and styling.","type":"text"}],"role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10097-Advances-in-UICollectionView","type":"topic"},"doc://WWDCNotes/documentation/WWDCNotes/donnywals":{"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/donnywals","abstract":[{"type":"text","text":"No Bio on GitHub"}],"role":"sampleCode","type":"topic","title":"Donny Wals (3 notes)","url":"\/documentation\/wwdcnotes\/donnywals"},"doc://WWDCNotes/documentation/WWDCNotes/elkraneo":{"kind":"article","identifier":"