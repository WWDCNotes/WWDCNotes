ion?","}","","@Model","class BucketListItem {...}","","@Model","class LivingAccommodation {...}"]},{"text":"Customize the schema","anchor":"Customize-the-schema","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Macros for customizing the models:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"text":"Use ","type":"text"},{"code":"@Attributes","type":"codeVoice"},{"text":" to set properties for a model property","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"Use ","type":"text"},{"code":"@Relationships","type":"codeVoice"},{"text":" for relations to other models","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"User ","type":"text"},{"code":"@Transient","type":"codeVoice"},{"text":" to avoid persisting a property","type":"text"}],"type":"paragraph"}]}]},{"text":"Unique macro","anchor":"Unique-macro","level":3,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Ensure data uniqueness by using the "},{"code":"#Unique","type":"codeVoice"},{"type":"text","text":" macro:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"text":"Unique property ","type":"text"},{"inlineContent":[{"text":"combination","type":"text"}],"type":"strong"},{"text":".","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Prevents duplicated entries."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Support of "},{"type":"strong","inlineContent":[{"type":"text","text":"upsert"}]},{"type":"text","text":" (update\/insert) operations."}],"type":"paragraph"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"Use the new "},{"type":"codeVoice","code":"#Unique"},{"type":"text","text":" macro to tell SwiftData which combinations of your model’s properties must always remain unique in the model data."}]}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"When two model instances share the same unique values, SwiftData will perform an upsert on collision with an existing model.","type":"text"}],"type":"emphasis"}]}],"style":"note","type":"aside","name":"Note"},{"type":"codeListing","syntax":"swift","code":["\/\/ Add unique constraints to avoid duplication","import SwiftData","","@Model ","class Trip {","    #Unique<Trip>([\\.name, \\.startDate, \\.endDate])","    ","    var name: String","    var destination: String","    var startDate: Date","    var endDate: Date","    ","    var bucketList: [BucketListItem] = [BucketListItem]()","    var livingAccommodation: LivingAccommodation?","}"]},{"type":"paragraph","inlineContent":[{"text":"Add the ","type":"text"},{"code":"@Attribute(.preserveValueOnDeletion)","type":"codeVoice"},{"text":" to the properties to ensure these identity values will be available when using the history API in SwiftData.","type":"text"}]},{"text":"Reveal history with SwiftData","anchor":"Reveal-history-with-SwiftData","level":3,"type":"heading"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Track inserted, updated, and deleted models"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Opt in to preserve values on deletion with the ","type":"text"},{"code":"#Preserve","type":"codeVoice"},{"text":" macro.","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"inlineContent":[{"text":"Tombstone values","type":"text"}],"type":"strong"},{"type":"text","text":" for deleted models."}],"type":"paragraph"}]}],"type":"unorderedList"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Works with custom data stores"}]}]},{"content":[{"inlineContent":[{"text":"Useful for auditing and processing changes.","type":"text"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"text":"Learn more in the session: “Track model changes with SwiftData history”","type"