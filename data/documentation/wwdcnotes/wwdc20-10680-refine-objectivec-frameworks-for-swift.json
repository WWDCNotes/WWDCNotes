{"kind":"article","schemaVersion":{"patch":0,"minor":3,"major":0},"sampleCodeDownload":{"kind":"sampleDownload","action":{"identifier":"https:\/\/developer.apple.com\/wwdc20\/10680","isActive":true,"overridingTitle":"Watch Video (42 min)","type":"reference"}},"primaryContentSections":[{"content":[{"text":"Overview","level":2,"anchor":"overview","type":"heading"},{"inlineContent":[{"type":"codeVoice","code":"swiftc"},{"type":"text","text":"’s Clang Importer automatically generates interfaces for Objective-C frameworks, for example:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"NSString","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"NSDate","type":"codeVoice"},{"text":" parameters are bridged into ","type":"text"},{"code":"String","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"Date","type":"codeVoice"},{"text":" structs","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"all "},{"type":"codeVoice","code":"init"},{"type":"text","text":" methods are imported as initializers"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"all methods are rewritten into a style closer to Swift","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"methods that follow the Objective-C error handling convention are turned into throwing functions"}]}]}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"For example, we go from:","type":"text"}],"type":"paragraph"},{"code":["\/\/ SKMission.h ","","#import <Foundation\/Foundation.h>","#import <SpaceKit\/SKAstronaut.h>","#import <SpaceKit\/SKCapsule.h>","#import <SpaceKit\/SKRocket.h>","","@interface SKMission NSObject","","- (instancetype)initWithName:(NSString *)name ","                  launchDate:(NSDate *) launchDate ","                      rocket:(NSString *)rocket","                     capsule:(NSString *) capsule;","- (instancetype)initWithContentsofURL:(NSURL*)url","                                error:(NSError**)error; ","@property (copy) NSString *name;","@property (strong) NSDate *launchDate;","@property (copy) NSString *rocket;","@property (copy) NSString *capsule;","@property (copy) NSArray *crew;","","\/\/\/\\returns \\c YES if saved; \\c NO with non-nil \\c *error if failed to save;","\/\/\/         \\c NO with nil \\c *error' if nothing needed to be saved.","- (BOOL) saveToURL:(NSURL *)url ","             error: (NSError **) error;"],"syntax":"objc","type":"codeListing"},{"inlineContent":[{"type":"text","text":"To:"}],"type":"paragraph"},{"code":["open class SKMission: NSObject {","  public init!(name: String!, launchDate: Date!, rocket: String!, capsule: String!)","  public init (contentsOf url: URL!) throws","","  open var name: String! { get } ","  open var launchDate: Date! { get }","  open var rocket: String! { get }","  open var capsule: String! { get }","  open var crew: [Any]! { get } ","","  \/\/\/ \\returns \\c YES if saved; \\c NO with non-nil \\c *error if failed to save; \\c NO with ","  \/\/\/ nil \\c *error if nothing needed to be saved. ","  open func save(to url: URL!) throws ","","  open func previousMissionsFlown(by astronaut: SKAstronaut!) -> Set ‹AnyHashable>! ","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"There’s room for improvements from the generated interface:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"the API has many implicitly unwrapped optionals"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"Any"},{"type":"text","text":" and "},{"type":"codeVoice","code":"AnyHashable"},{"type":"text","text":" types are vague"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"the ","type":"text"},{"code":"throws","type":"codeVoice"},{"text":" method will sometimes throw when it shouldn’t","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"some method names could also be more swifty","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"and more","type":"text"}]}]}],"type":"unorderedList"},{"text":"Provide richer type information","level":2,"anchor":"Provide-richer-type-information","type":"heading"},{"text":"Describe nullability to control optionals","level":3,"anchor":"Describe-nullability-to-control-optionals","type":"heading"},{"inlineContent":[{"type":"text","text":"When Swift imports an Objective-C pointer type, by default, it marks it as an implicitly unwrapped optional to tell you that this value could be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"."}],"type":"paragraph"},{"text":"Methods and Properties","level":4,"anchor":"Methods-and-Properties","type":"heading"},{"inlineContent":[{"text":"Objective-C provides three nullability annotations, ","type":"text"},{"type":"codeVoice","code":"nonnull"},{"text":", ","type":"text"},{"type":"codeVoice","code":"nullable"},{"text":", ","type":"text"},{"type":"codeVoice","code":"null_unspecified"},{"text":", which let you say whether ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" is a sensible value for a particular property, method parameter or method result:","type":"text"}],"type":"paragraph"},{"code":["\/\/ SKMission.h","","#import <Foundation\/Foundation.h>","","@interface SKMission : NSObject","","@property (readonly, nullable) NSString *name;","","- (nonnull instancetype)initWithName:(nullable NSString *)name;","","@end"],"syntax":"objc","type":"codeListing"},{"name":"Note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Objective-C doesn’t enforce these annotations. They just document your intent."}]}],"style":"note","type":"aside"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"nonnull"},{"text":" will be imported as a non-optional type in Swift","type":"text"}]}]},{"content":[{"inlineContent":[{"code":"nullable","type":"codeVoice"},{"text":" will be imported as an optional type in Swift","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"null_unspecified","type":"codeVoice"},{"type":"text","text":" will be imported as an implicitly unwrapped optional"}]}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Lastly, add the "},{"type":"codeVoice","code":"NS_ASSUME_NONNULL_BEGIN"},{"type":"text","text":" macro at the top of the header file and the matching end macro at the bottom, then delete all the "},{"type":"codeVoice","code":"nonnull"},{"type":"text","text":"s between them."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This is a convenience macro to save you typing "},{"type":"codeVoice","code":"nonnull"},{"type":"text","text":" in your headers."}],"type":"paragraph"},{"code":["\/\/ SKMission.h","","#import <Foundation\/Foundation.h>","","NS_ASSUME_NONNULL_BEGIN","","@interface SKMission : NSObject","","@property (readonly, nullable) NSString *name;","","- (instancetype)initWithName:(nullable NSString *)name;","","@end","","NS_ASSUME_NONNULL_END"],"syntax":"objc","type":"codeListing"},{"text":"Any Pointer","level":4,"anchor":"Any-Pointer","type":"heading"},{"inlineContent":[{"text":"For other pointers that are not methods and properties, use ","type":"text"},{"code":"_Nonnull","type":"codeVoice"},{"text":", ","type":"text"},{"code":"_Nullable","type":"codeVoice"},{"text":", and ","type":"text"},{"code":"_Null_unspecified","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"code":["\/\/ Misc.h","","#import <Foundation\/Foundation.h>","","NSString * _Nonnull const SKRocketSaturnV;","","@interface ResourceValueContainer : NSObject","","- (BOOL)getResourceValue:(id _Nullable * _Nonnull)outValue error:(NSError**)error;","","@end"],"syntax":"objc","type":"codeListing"},{"text":"Nullability mistakes","level":4,"anchor":"Nullability-mistakes","type":"heading"},{"inlineContent":[{"text":"What happens when Objective-C returns ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" for a value Swift thinks can’t be optional?","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"If it’s an "},{"code":"NSString","type":"codeVoice"},{"type":"text","text":" or "},{"code":"NSArray","type":"codeVoice"},{"type":"text","text":" on the Objective-C side, you get an empty Swift string or array"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"If it’s an Objective-C object, you might not even notice because Objective-C method calls ignore nils. But in some cases, you’ll crash with a null pointer dereference or get other unexpected behavior"}],"type":"paragraph"}]}],"type":"unorderedList"},{"text":"Use Objective-C generics for Foundation types","level":3,"anchor":"Use-Objective-C-generics-for-Foundation-types","type":"heading"},{"code":["\/\/ SKMission.h","","#import <Foundation\/Foundation.h>","#import <SpaceKit\/SKAstronaut.h>","","NS_ASSUME_NONNULL_BEGIN","","@interface SKMission : NSObject","","@property (readonly) NSArray<SKAstronaut *> *crew;","","@end","","NS_ASSUME_NONNULL_END"],"syntax":"objc","type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"NSArray<SKAstronaut *>"},{"text":" will translate into ","type":"text"},{"type":"codeVoice","code":"[SKAstronaut]"}],"type":"paragraph"},{"text":"Use `Int` for numbers","level":3,"anchor":"Use-Int-for-numbers","type":"heading"},{"inlineContent":[{"text":"In both Objective-C and Swift, it’s conventional to use unsigned types (e.g. ","type":"text"},{"type":"codeVoice","code":"UInt"},{"text":", ","type":"text"},{"type":"codeVoice","code":"uint8_t"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt8"},{"text":") when an integer represents a collection of bits and you want to perform bitwise operations on those bits.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The main reason people use "},{"type":"codeVoice","code":"NSUInteger"},{"type":"text","text":" in Objective-C is to indicate that a number’s value is never negative."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Objective-C enables this style with automatic conversions and carefully designed overflow behaviors, but these exact features can cause serious security bugs, so Swift doesn’t include them."}],"type":"paragraph"},{"inlineContent":[{"text":"Instead, Swift requires you to explicitly convert unsigned types to signed if you wanted signed arithmetic, and stops execution if unsigned arithmetic would produce a negative result.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Apple’s recommendation (and what they do in their frameworks): turn all "},{"code":"NSUIntegers","type":"codeVoice"},{"type":"text","text":" into "},{"code":"Int","type":"codeVoice"},{"type":"text","text":"s when Swift imports them."}],"type":"paragraph"},{"text":"Strengthen stringly-typed constants","level":3,"anchor":"Strengthen-stringly-typed-constants","type":"heading"},{"inlineContent":[{"type":"text","text":"A "},{"type":"codeVoice","code":"typedef"},{"type":"text","text":" gets imported as a type-alias in Swift, and in both languages, that’s just an exact synonym for the original type."}],"type":"paragraph"},{"inlineContent":[{"text":"However, when we add the ","type":"text"},{"code":"NS_STRING_ENUM","type":"codeVoice"},{"text":" macro after the ","type":"text"},{"code":"typedef","type":"codeVoice"},{"text":", this dramatically reshapes the Swift translation:","type":"text"},{"text":"\n","type":"text"},{"text":"it now imports as a struct with the constants nested inside it, making something that looks and feels just like an enum with a raw string value.","type":"text"}],"type":"paragraph"},{"code":["\/\/ SKRocket.h","","#import <Foundation\/Foundation.h>","","NS_ASSUME_NONNULL_BEGIN","","typedef NSString *SKRocket NS_STRING_ENUM;","","extern SKRocket const SKRocketAtlas;","extern SKRocket const SKRocketTitanII;","extern SKRocket const SKRocketSaturnIB;","extern SKRocket const SKRocketSaturnV;","","NSInteger SKRocketStageCount(SKRocket);","","NS_ASSUME_NONNULL_END"],"syntax":"objc","type":"codeListing"},{"code":["public struct SKRocket: RawRepresentable {","  public var rawValue: String","  public static let atlas: SKRocket","  public static let titanII: SKRocket","  public static let saturnIB: SKRocket","  public static let saturnV: SKRocket","}","","public func SKRocketStageCount(_: SKRocket) -> Int"],"syntax":"swift","type":"codeListing"},{"text":"Follow Objective-C conventions","level":2,"anchor":"Follow-Objective-C-conventions","type":"heading"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"use ","type":"text"},{"code":"NS_DESIGNATED_INITIALIZER","type":"codeVoice"},{"text":" for the main initializer of a class (other initializers will automatically get be tagged as ","type":"text"},{"code":"convenience","type":"codeVoice"},{"text":" in Swift)","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"use "},{"code":"NS_UNAVAILABLE","type":"codeVoice"},{"type":"text","text":" for initializers that you don’t support (e.g. "},{"code":"NSObject","type":"codeVoice"},{"type":"text","text":"‘s "},{"code":"init","type":"codeVoice"},{"type":"text","text":" when you don’t want to override it and want people to use your initializers instead)"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"use the Objective-C error convention correctly, use ","type":"text"},{"code":"NS_SWIFT_NOTHROW","type":"codeVoice"},{"text":" otherwise","type":"text"}]}]}],"type":"unorderedList"},{"code":["\/\/ SKMission.h","#import <Foundation\/Foundation.h>","","NS_ASSUME_NONNULL_BEGIN","","@interface SKMission : NSObject","","\/\/\/ \\returns \\c YES if saved; \\c NO with non-nil \\c *error if failed to save;","\/\/\/          \\c NO with nil \\c *error` if nothing needed to be saved.","- (BOOL)saveToURL:(NSURL *)url error:(NSError **)error NS_SWIFT_NOTHROW DEPRECATED_ATTRIBUTE;","","\/\/\/ @param[out] wasDirty If provided, set to \\c YES if the file needed to be","\/\/\/   saved or \\c NO if there weren’t any changes to save.","- (BOOL)saveToURL:(NSURL *)url ","         wasDirty:(nullable BOOL *)wasDirty ","           error:(NSError **)error;","","@end","","NS_ASSUME_NONNULL_END"],"syntax":"objc","type":"codeListing"},{"inlineContent":[{"type":"text","text":"Will turn into:"}],"type":"paragraph"},{"code":["Class SKMission: NSObject {","  @available(*, deprecated)","  public func save(to url: URL, error: AutoreleasingUnsafeMutablePointer..)","","  public func save(to url: URL, wasDirty: UnsafeMutablePointer<ObjCBool>?) throws","}"],"syntax":"swift","type":"codeListing"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Use ","type":"text"},{"code":"NS_REFINED_FOR_SWIFT","type":"codeVoice"},{"text":" when you re-define a method in Swift and want to hide the original objc implementation (when imported into Swift)","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"this macro adds two underscores to the beginning of the method’s Swift name"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"When Xcode sees something with a leading underscore, it usually hides it from editor features like code completion and generated interfaces"}],"type":"paragraph"}]}]}]}],"type":"unorderedList"},{"text":"Address missing APIs","level":2,"anchor":"Address-missing-APIs","type":"heading"},{"inlineContent":[{"type":"text","text":"Swift can’t import:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"C-style variadic parameters","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Flexible array members","type":"text"}]}]},{"content":[{"inlineContent":[{"text":"Forward declarations (like an ","type":"text"},{"type":"codeVoice","code":"@class"},{"text":" or ","type":"text"},{"type":"codeVoice","code":"@protocol"},{"text":" with a semicolon) that are never fully defined","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"declarations involving un-importable types","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"invalid redeclarations"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"complicated macros"}]}]}],"type":"unorderedList"},{"text":"Improve ergonomics in Swift","level":2,"anchor":"Improve-ergonomics-in-Swift","type":"heading"},{"text":"Fix method names with `NS_SWIFT_NAME`","level":3,"anchor":"Fix-method-names-with-NSSWIFTNAME","type":"heading"},{"inlineContent":[{"text":"Instead of","type":"text"}],"type":"paragraph"},{"code":["- (NSSet<SKMission *> *)previousMissionsFlownByAstronaut:(SKAstronaut *)astronaut;"],"syntax":"objc","type":"codeListing"},{"inlineContent":[{"type":"text","text":"that turns into:"}],"type":"paragraph"},{"code":["func previousMissionsFlown(by astronaut: SKAstronaut) -> Set<SKMission>"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"Use ","type":"text"},{"code":"NS_SWIFT_NAME","type":"codeVoice"},{"text":":","type":"text"}],"type":"paragraph"},{"code":["- (NSSet<SKMission *> *)previousMissionsFlownByAstronaut:(SKAstronaut *)astronaut NS_SWIFT_NAME(previousMissions(flownBy:));"],"syntax":"objc","type":"codeListing"},{"inlineContent":[{"text":"that becomes:","type":"text"}],"type":"paragraph"},{"code":["func previousMissions(flownBy astronaut: SKAstronaut) -> Set<SKMission>"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"NS_SWIFT_NAME"},{"text":" is very powerful, for example it can be used to change a global function into a static\/instance function of a type","type":"text"}],"type":"paragraph"},{"text":"Error code enums","leve