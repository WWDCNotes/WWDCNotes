tation/WWDCNotes/WWDC20-10647-Become-a-Simulator-expert":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10647-Become-a-Simulator-expert","title":"Become a Simulator expert","type":"topic","url":"\/documentation\/wwdcnotes\/wwdc20-10647-become-a-simulator-expert","abstract":[{"text":"Simulator runs your iOS, iPadOS, tvOS, or watchOS apps directly on your Mac — no separate device required. We’ll give you a tour of the app’s latest tools and features and show you how to sharpen your Simulator skills. Discover how to test pointer and trackpad support, adjust Simulator preferences, and use command line tools like simctl to help you simulate push notifications in a development environment.","type":"text"}],"kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC19-262-Embedding-and-Sharing-Visually-Rich-Links":{"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC19-262-Embedding-and-Sharing-Visually-Rich-Links","title":"Embedding and Sharing Visually Rich Links","url":"\/documentation\/wwdcnotes\/wwdc19-262-embedding-and-sharing-visually-rich-links","role":"sampleCode","type":"topic","abstract":[{"type":"text","text":"The new Link Presentation framework enables app developers to easily present URLs in a rich, beautiful, and consistent way. Learn how to use Link Presentation to retrieve metadata from a URL, present the rich link content inside your app, and provide link metadata to the new share sheet experience in iOS."}]},"doc://WWDCNotes/documentation/WWDCNotes/marcolg97":{"url":"\/documentation\/wwdcnotes\/marcolg97","title":"Marco La Gala (1 note)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/marcolg97","abstract":[{"text":"iOS Developer - in love with SwiftUI","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/multitudes":{"url":"\/documentation\/wwdcnotes\/multitudes","title":"laurent b (32 notes)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/multitudes","abstract":[{"text":"student at 42Berlin 🐬 | 🍎 Swift(UI) app dev  | speciality coffee ☕️ & cycling 🚴🏻‍♂️","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/lordmooch":{"url":"\/documentation\/wwdcnotes\/lordmooch","title":"Michael Lysons (2 notes)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/lordmooch","abstract":[{"text":"iOS developer. Swift. SwiftUI. SQLite. MS SQL Server.","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/ATahhan":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/ATahhan","title":"Ammar AlTahhan (11 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/atahhan","abstract":[{"text":"Software Engineer","type":"text"}],"kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/mackuba":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/mackuba","abstract":[{"text":"Independent Mac & iOS developer. Sometimes freelancing, mostly working on my own stuff these days.","type":"text"}],"title":"Kuba Suder (7 notes)","url":"\/documentation\/wwdcnotes\/mackuba","type":"topic","role":"sampleCode","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/stevenpaulhoward":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/stevenpaulhoward","title":"stevenpaulhoward (4 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/stevenpaulhoward","abstract":[{"text":"3D connoisseur","type":"text"}],"kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10217-Explore-numerical-computing-in-Swift":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10217-Explore-numerical-computing-in-Swift","type":"topic","url":"\/documentation\/wwdcnotes\/wwdc20-10217-explore-numerical-computing-in-swift","title":"Explore numerical computing in Swift","role":"sampleCode","abstract":[{"text":"Meet Swift Numerics: a new Swift package for computational mathematics. Take a tour of the protocols and types available in the package and find out how you can use them to write generic code. We’ll also show you how and when to use the new Float16 type to improve performance and reduce memory usage.","type":"text"}],"kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/kslazinski":{"url":"\/documentation\/wwdcnotes\/kslazinski","title":"Kris Slazinski (2 notes)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/kslazinski","abstract":[{"text":"UX Designer @next-planet","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/Cecile-Lebleu":{"type":"topic","url":"\/documentation\/wwdcnotes\/cecile-lebleu","title":"Cécile Lebleu (3 notes)","abstract":[{"text":"Designer turned developer.","type":"text"}],"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/Cecile-Lebleu","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/DaemonLoki":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/DaemonLoki","abstract":[{"text":"Machine Learning and AI enthusiast with a passion for mobile app development.","type":"text"}],"title":"Stefan Blos (1 note)","url":"\/documentation\/wwdcnotes\/daemonloki","type":"topic","role":"sampleCode","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/chrisvasselli":{"url":"\/documentation\/wwdcnotes\/chrisvasselli","title":"Chris Vasselli (1 note)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/chrisvasselli","abstract":[{"text":"Indie app developer, dad, creator of","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/Jeehut":{"role":"sampleCode","url":"\/documentation\/wwdcnotes\/jeehut","kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/Jeehut","type":"topic","abstract":[{"type":"text","text":"Spatial-first Indie Developer for  Platforms. Actively contributing to Open Source since 2011!"}],"title":"Cihat Gündüz (54 notes)"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10206-Designed-for-iPad":{"type":"topic","abstract":[{"text":"Discover the building blocks for designing a great iPad app: Learn how to minimize use of modal interfaces and leverage the new sidebar to increase efficiency by streamlining navigation and facilitating powerful drag and drop interactions. See how to take advantage of iPad’s versatile interaction opportunities by supporting multitouch, pencil, keyboard, and trackpad. And create adaptive layouts that respond to all size classes and orientation to support a great multitasking experience. These techniques will not only improve your iPad app — they’ll make it easier to create a Mac Catalyst app that truly feels right at home on macOS.","type":"text"}],"url":"\/documentation\/wwdcnotes\/wwdc20-10206-designed-for-ipad","title":"Designed for iPad","role":"sampleCode","kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10206-Designed-for-iPad"},"doc://WWDCNotes/documentation/WWDCNotes/pitt500":{"url":"\/documentation\/wwdcnotes\/pitt500","title":"Pedro Rojas (1 note)","type":"topic","abstract":[{"text":"iOS & Swift Specialist | Content Creator | Engineering Manager at Insulet | Formerly Meta and HP","type":"text"}],"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/pitt500","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10160-Formatters-Make-data-humanfriendly":{"url":"\/documentation\/wwdcnotes\/wwdc20-10160-formatters-make-data-humanfriendly","title":"Formatters: Make data human-friendly","abstract":[{"text":"Save yourself time and frustration: When you display data in your app — including dates, times, measurements, names, lists, numbers, or strings — learn how to format it correctly and provide a great experience. We’ll walk you through the Formatter APIs as well as how SwiftUI works with stringsdict, and show you how they can help do the heavy lifting of formatting data. Learn about best practices and how to avoid common mistakes.","type":"text"}],"type":"topic","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10160-Formatters-Make-data-humanfriendly","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10611-Explore-ARKit-4":{"type":"topic","title":"Explore ARKit 4","url":"\/documentation\/wwdcnotes\/wwdc20-10611-explore-arkit-4","role":"sampleCode","abstract":[{"type":"text","text":"ARKit 4 enables you to build the next generation of augmented reality apps to transform how people connect with the world around them. We’ll walk you through the latest improvements to Apple’s augmented reality platform, including how to use Location Anchors to connect virtual objects with a real-world longitude, latitude, and altitude. Discover how to harness the LiDAR Scanner on iPad Pro and obtain a depth map of your environment. And learn how to track faces in AR on more devices, including the iPad Air (3rd generation), iPad mini (5th generation), and all devices with the A12 Bionic chip or later that have a front-facing camera."}],"kind":"article","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10611-Explore-ARKit-4"},"doc://WWDCNotes/documentation/WWDCNotes/davidleee":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/davidleee","title":"David Lee (2 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/davidleee","abstract":[{"text":"Indie iOS developer🧑🏻‍💻","type":"text"}],"kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/thecodedself":{"url":"\/documentation\/wwdcnotes\/thecodedself","title":"Keegan Rush (1 note)","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/thecodedself","abstract":[{"text":"A software developer that likes building iOS apps, doing confusing things with Git, and attempting to find the Holy Grail of software development efficacy.","type":"text"}],"type":"topic","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10111-Boost-performance-and-security-with-modern-networking":{"url":"\/documentation\/wwdcnotes\/wwdc20-10111-boost-performance-and-security-with-modern-networking","title":"Boost performance and security with modern networking","abstract":[{"text":"Speed up your app and make it more nimble, private and secure with modern networking APIs. Learn about networking protocols like IPv6, HTTP\/2, TLS 1.3 and Encrypted DNS, and how incorporating these within your app and server can provide faster performance and reduce both your power consumption and thermal impact. In addition, discover how adopting the latest security protocols can help you better protect privacy within your app.","type":"text"}],"type":"topic","role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10111-Boost-performance-and-security-with-modern-networking","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/srujanc":{"kind":"article","title":"Srujan Chitla (5 notes)","abstract":[{"type":"text","text":"Sr Software Engineer at IQVIA"}],"role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/srujanc","url":"\/documentation\/wwdcnotes\/srujanc","type":"topic"}}}