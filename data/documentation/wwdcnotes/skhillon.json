/WWDCNotes/documentation/WWDCNotes/HARlBO":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/HARlBO","title":"Jinha (1 note)","type":"topic","url":"\/documentation\/wwdcnotes\/harlbo","role":"sampleCode","kind":"article","abstract":[{"type":"text","text":"𝗶𝗢𝗦 𝗗𝗲𝘃𝗲𝗹𝗼𝗽𝗲𝗿📱"}]},"doc://WWDCNotes/documentation/WWDCNotes/JohnBaer3":{"title":"John Baer (2 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/johnbaer3","kind":"article","role":"sampleCode","abstract":[{"text":"No Bio on GitHub","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/JohnBaer3"},"doc://WWDCNotes/documentation/WWDCNotes/ronyfadel":{"role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/ronyfadel","abstract":[{"type":"text","text":"Former Apple engineer turned indie developer."}],"url":"\/documentation\/wwdcnotes\/ronyfadel","type":"topic","title":"Rony Fadel (2 notes)","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes/RamitSharma991":{"title":"Ramit Sharma (11 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/ramitsharma991","kind":"article","role":"sampleCode","abstract":[{"text":"Indie iOS Dev. Swift, SwiftUI, Obj-C, UX and related.","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/RamitSharma991"},"doc://WWDCNotes/documentation/WWDCNotes/rayfix":{"abstract":[{"text":"No Bio on GitHub","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/rayfix","type":"topic","role":"sampleCode","title":"Ray Fix (1 note)","kind":"article","url":"\/documentation\/wwdcnotes\/rayfix"},"doc://WWDCNotes/documentation/WWDCNotes/thecodedself":{"title":"Keegan Rush (1 note)","type":"topic","url":"\/documentation\/wwdcnotes\/thecodedself","kind":"article","role":"sampleCode","abstract":[{"type":"text","text":"A software developer that likes building iOS apps, doing confusing things with Git, and attempting to find the Holy Grail of software development efficacy."}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/thecodedself"},"doc://WWDCNotes/documentation/WWDCNotes/Cecile-Lebleu":{"title":"Cécile Lebleu (3 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/cecile-lebleu","kind":"article","role":"sampleCode","abstract":[{"text":"Designer turned developer.","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/Cecile-Lebleu"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10647-Become-a-Simulator-expert":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10647-Become-a-Simulator-expert","url":"\/documentation\/wwdcnotes\/wwdc20-10647-become-a-simulator-expert","role":"sampleCode","kind":"article","title":"Become a Simulator expert","abstract":[{"type":"text","text":"Simulator runs your iOS, iPadOS, tvOS, or watchOS apps directly on your Mac — no separate device required. We’ll give you a tour of the app’s latest tools and features and show you how to sharpen your Simulator skills. Discover how to test pointer and trackpad support, adjust Simulator preferences, and use command line tools like simctl to help you simulate push notifications in a development environment."}]},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10206-Designed-for-iPad":{"url":"\/documentation\/wwdcnotes\/wwdc20-10206-designed-for-ipad","role":"sampleCode","kind":"article","type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10206-Designed-for-iPad","abstract":[{"type":"text","text":"Discover the building blocks for designing a great iPad app: Learn how to minimize use of modal interfaces and leverage the new sidebar to increase efficiency by streamlining navigation and facilitating powerful drag and drop interactions. See how to take advantage of iPad’s versatile interaction opportunities by supporting multitouch, pencil, keyboard, and trackpad. And create adaptive layouts that respond to all size classes and orientation to support a great multitasking experience. These techniques will not only improve your iPad app — they’ll make it easier to create a Mac Catalyst app that truly feels right at home on macOS."}],"title":"Designed for iPad"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10111-Boost-performance-and-security-with-modern-networking":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10111-Boost-performance-and-security-with-modern-networking","url":"\/documentation\/wwdcnotes\/wwdc20-10111-boost-performance-and-security-with-modern-networking","role":"sampleCode","kind":"article","title":"Boost performance and security with modern networking","abstract":[{"type":"text","text":"Speed up your app and make it more nimble, private and secure with modern networking APIs. Learn about networking protocols like IPv6, HTTP\/2, TLS 1.3 and Encrypted DNS, and how incorporating these within your app and server can provide faster performance and reduce both your power consumption and thermal impact. In addition, discover how adopting the latest security protocols can help you better protect privacy within your app."}]},"doc://WWDCNotes/documentation/WWDCNotes/mackuba":{"title":"Kuba Suder (7 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/mackuba","kind":"article","role":"sampleCode","abstract":[{"type":"text","text":"Independent Mac & iOS developer. Sometimes freelancing, mostly working on my own stuff these days."}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/mackuba"},"doc://WWDCNotes/documentation/WWDCNotes/kyaw-codes":{"title":"Kyaw Zay Ya Lin Tun (3 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/kyaw-codes","kind":"article","role":"sampleCode","abstract":[{"text":"Senior iOS Developer @sgcodigo | Programming Mentor | Swift enthusiast","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/kyaw-codes"},"doc://WWDCNotes/documentation/WWDCNotes/davidleee":{"title":"David Lee (2 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/davidleee","kind":"article","role":"sampleCode","abstract":[{"text":"Indie iOS developer🧑🏻‍💻","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/davidleee"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10167-Safely-manage-pointers-in-Swift":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10167-Safely-manage-pointers-in-Swift","role":"sampleCode","title":"Safely manage pointers in Swift","kind":"article","type":"topic","url":"\/documentation\/wwdcnotes\/wwdc20-10167-safely-manage-pointers-in-swift","abstract":[{"text":"Come with us as we delve into unsafe pointer types in Swift. Discover the requirements for each type and how to use it correctly. We’ll discuss typed pointers, drop down to raw pointers, and finally circumvent pointer type safety entirely by binding memory.","type":"text"}]},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10687-Triage-test-failures-with-XCTIssue":{"abstract":[{"type":"text","text":"Put your test failures to work: Learn how to triage and diagnose uncaught issues in your app using the latest testing APIs in Xcode. We’ll show you how to help ease your testing workflow and put failures into context to help you deliver the best quality product."}],"role":"sampleCode","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10687-Triage-test-failures-with-XCTIssue","type":"topic","title":"Triage test failures with XCTIssue","url":"\/documentation\/wwdcnotes\/wwdc20-10687-triage-test-failures-with-xctissue","kind":"article"},"doc://WWDCNotes/documentation/WWDCNotes":{"images":[{"type":"icon","identifier":"WWDCNotes.png"}],"title":"WWDC Notes","abstract":[{"type":"text","text":"Session notes shared by the community for the community."}],"type":"topic","kind":"symbol","url":"\/documentation\/wwdcnotes","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes","role":"collection"},"doc://WWDCNotes/documentation/WWDCNotes/halmueller":{"abstract":[{"type":"text","text":"No Bio on GitHub"}],"title":"Hal Mueller (2 notes)","kind":"article","url":"\/documentation\/wwdcnotes\/halmueller","type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/halmueller","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10099-Explore-the-Action-and-Vision-app":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10099-Explore-the-Action-and-Vision-app","url":"\/documentation\/wwdcnotes\/wwdc20-10099-explore-the-action-and-vision-app","role":"sampleCode","kind":"article","title":"Explore the Action & Vision app","abstract":[{"type":"text","text":"It’s now easy to create an app for fitness or sports coaching that takes advantage of machine learning — and to prove it, we built our own. Learn how we designed the Action & Vision app using Object Detection and Action Classification in Create ML along with the new Body Pose Estimation, Trajectory Detection, and Contour Detection features in the Vision framework. Explore how you can create an immersive application for gameplay or training from setup to analysis and feedback. And follow along in Xcode with a full sample project."}]},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10217-Explore-numerical-computing-in-Swift":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10217-Explore-numerical-computing-in-Swift","url":"\/documentation\/wwdcnotes\/wwdc20-10217-explore-numerical-computing-in-swift","role":"sampleCode","kind":"article","title":"Explore numerical computing in Swift","abstract":[{"type":"text","text":"Meet Swift Numerics: a new Swift package for computational mathematics. Take a tour of the protocols and types available in the package and find out how you can use them to write generic code. We’ll also show you how and when to use the new Float16 type to improve performance and reduce memory usage."}]},"doc://WWDCNotes/documentation/WWDCNotes/chrisvasselli":{"abstract":[{"text":"Indie app developer, dad, creator of","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/chrisvasselli","type":"topic","role":"sampleCode","title":"Chris Vasselli (1 note)","kind":"article","url":"\/documentation\/wwdcnotes\/chrisvasselli"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC20-10165-Embrace-Swift-type-inference":{"url":"\/documentation\/wwdcnotes\/wwdc20-10165-embrace-swift-type-inference","role":"sampleCode","kind":"article","type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC20-10165-Embrace-Swift-type-inference","abstract":[{"type":"text","text":"Swift uses type inference to help you write clean, concise code without compromising type safety. We’ll show you how the compiler seeks out clues in your code to solve the type inference puzzle. Discover what happens when the compiler can’t come to a solution, and find out how Xcode 12 integrates error tracking to help you understand and fix mistakes at compile time."}],"title":"Embrace Swift type inference"},"doc://WWDCNotes/documentation/WWDCNotes/spanage":{"title":"Sommer Panage (1 note)","type":"topic","url":"\/documentation\/wwdcnotes\/spanage","kind":"article","role":"sampleCode","abstract":[{"text":"SwiftUI EM @ Apple, iOS Eng, Accessibility specialist – formerly Slack and Twitter.","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/spanage"},"doc://WWDCNotes/documentation/WWDCNotes/rakeshneela":{"title":"Rakesh Neela (1 note)","type":"topic","url":"\/documentation\/wwdcnotes\/rakeshneela","kind":"article","role":"sampleCode","abstract":[{"text":"No Bio on GitHub","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/rakeshneela"},"doc://WWDCNotes/documentation/WWDCNotes/multitudes":{"title":"laurent b (32 notes)","type":"topic","url":"\/documentation\/wwdcnotes\/multitudes","kind":"article","role":"sampleCode","abstract":[{"text":"student at 42Berlin 🐬 | 🍎 Swift(UI) app dev  | speciality coffee ☕️ & cycling 🚴🏻‍♂️","type":"text"}],"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/multitudes"}}}