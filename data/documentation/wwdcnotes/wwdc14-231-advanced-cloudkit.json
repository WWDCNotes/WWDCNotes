er.apple.com/documentation/cloudkit/ckfetchrecordchangesoperation":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordchangesoperation","title":"CKFetchRecordChangesOperation","titleInlineContent":[{"type":"codeVoice","code":"CKFetchRecordChangesOperation"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordchangesoperation","type":"link"},"doc://WWDCNotes/documentation/WWDCNotes/WWDC14":{"type":"topic","identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC14","abstract":[{"text":"Xcode 6, Swift 1.0, iOS 8, macOS 10.10 (Yosemite).","type":"text"}],"kind":"article","images":[{"identifier":"WWDCNotes.png","type":"icon"}],"title":"WWDC14","url":"\/documentation\/wwdcnotes\/wwdc14","role":"collectionGroup"},"https://developer.apple.com/documentation/cloudkit/ckoperation":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckoperation","title":"CKOperation","titleInlineContent":[{"type":"codeVoice","code":"CKOperation"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckoperation","type":"link"},"https://developer.apple.com/documentation/cloudkit/ckfetchrecordsoperation":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordsoperation","title":"CKFetchRecordsOperation","titleInlineContent":[{"type":"codeVoice","code":"CKFetchRecordsOperation"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordsoperation","type":"link"},"https://wwdcnotes.github.io/WWDCNotes/documentation/wwdcnotes/contributing":{"identifier":"https:\/\/wwdcnotes.github.io\/WWDCNotes\/documentation\/wwdcnotes\/contributing","title":"Contributions are welcome!","titleInlineContent":[{"type":"text","text":"Contributions are welcome!"}],"url":"https:\/\/wwdcnotes.github.io\/WWDCNotes\/documentation\/wwdcnotes\/contributing","type":"link"},"https://developer.apple.com/documentation/cloudkit/ckerrorretryafterkey":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckerrorretryafterkey","title":"CKErrorRetryAfterKey","titleInlineContent":[{"type":"codeVoice","code":"CKErrorRetryAfterKey"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckerrorretryafterkey","type":"link"},"https://mackuba.eu":{"identifier":"https:\/\/mackuba.eu","title":"Blog","titleInlineContent":[{"type":"text","text":"Blog"}],"url":"https:\/\/mackuba.eu","type":"link"},"https://developer.apple.com/documentation/cloudkit/ckmodifyrecordsoperation/recordsavepolicy/allkeys":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckmodifyrecordsoperation\/recordsavepolicy\/allkeys","title":"SaveAllKeys","titleInlineContent":[{"type":"codeVoice","code":"SaveAllKeys"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckmodifyrecordsoperation\/recordsavepolicy\/allkeys","type":"link"},"WWDCNotes.png":{"identifier":"WWDCNotes.png","variants":[{"traits":["1x","light"],"url":"\/images\/WWDCNotes.png"}],"alt":null,"type":"image"},"https://x.com/kuba_suder":{"identifier":"https:\/\/x.com\/kuba_suder","title":"X\/Twitter","titleInlineContent":[{"type":"text","text":"X\/Twitter"}],"url":"https:\/\/x.com\/kuba_suder","type":"link"},"https://developer.apple.com/documentation/cloudkit/ckfetchrecordsoperation/3003361-desiredkeys":{"identifier":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordsoperation\/3003361-desiredkeys","title":"desiredKeys","titleInlineContent":[{"type":"codeVoice","code":"desiredKeys"}],"url":"https:\/\/developer.apple.com\/documentation\/cloudkit\/ckfetchrecordsoperation\/3003361-desiredkeys","type":"link"}}}