f,","            selection: $selectedStore","        ) {","            Text($0.name ?? \"Apple Store\")","        }","        .mapItemDetailSheet(item: $selectedStore)","    }","}"],"type":"codeListing"}],"title":"Swift"}]},{"inlineContent":[{"type":"text","text":"}"}],"type":"paragraph"},{"type":"heading","anchor":"Place-Card-API","level":2,"text":"Place Card API"},{"inlineContent":[{"inlineContent":[{"text":"Selection Accessory","type":"text"}],"type":"strong"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Swift - ","type":"text"},{"code":"MKSelectionAccessory","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"SwiftUI - ","type":"text"},{"code":"mapItemDetailSelectionAccessory","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"text":"Javascript - ","type":"text"},{"code":"PlaceSelectionAccessory","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"HTML - Web Embeds"}]}]}]},{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Map Feature Selection Accessory"}]}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Swift - "},{"type":"codeVoice","code":"selectableMapFeatures"},{"type":"text","text":" "},{"type":"codeVoice","code":"MKSelectionAccessory"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"SwiftUI - "},{"code":"MapFeature","type":"codeVoice"},{"type":"text","text":" "},{"code":"MapSelection","type":"codeVoice"},{"type":"text","text":" "},{"code":"mapFeatureSelectionAccessory","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"JavaScript - ","type":"text"},{"type":"codeVoice","code":"selectableMapFeatures"},{"text":" ","type":"text"},{"type":"codeVoice","code":"selectableMapFeaturesSelectionAccessory"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"HTML - Web Embeds"}]}]}]},{"type":"codeListing","syntax":"swift","code":["\/\/ Display a place card for selected map features","struct VisitedStoresView: View {","    var visitedStores: [MKMapItem]","        @State private var selection: MapSelection<MKMapItem>?","        ","        var body: some View {","            Map(selection: $selection) {","                ForEach(visitedStores, id: \\.self) { store in","                    Marker(item: store)","                        .tag(MapSelection(store))","            }","            .mapItemDetailSelectionAccessory(.callout)","        }","        .mapFeatureSelectionAccessory(.callout)","    }","}"]},{"type":"heading","anchor":"Finding-places","level":2,"text":"Finding places"},{"inlineContent":[{"text":"Find new kinds of places with more precision","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"Search Filters","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Place categories"}]}]},{"content":[{"inlineContent":[{"text":"Physical features","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Address components"}],"type":"paragraph"}]}]},{"inlineContent":[{"text":"Region priority","type":"text"},{"text":" ","type":"text"},{"text":"Pagination (Maps Server API)","type":"text"}],"type":"paragraph"},{"type":"heading","anchor":"Address-Filters","level":2,"text":"Address Filters"},{"type":"tabNavigator","tabs":[{"content":[{"type":"codeListing","syntax":"javascript","code":["\/\/ Find Cupertino, then find coffee (show-coffee.js)","window.initMapKit = () => {","    const addressFilter = mapkit.AddressFilter.including([","        mapkit.AddressCategory.Locality","    ]);","    const citySearch = new mapkit.Search({ addressFilter });","    citySearch.search(\"Cupertino\", showMap);","};","","\/\/ Found Cupertino, now find coffee","const showMap = (error, cities) => {","    const center = cities.places[0].coordinate;","    const span = new mapkit.CoordinateSpan(0.01, 0.01);","    const region = new mapkit.CoordinateRegion(center, span);","    const map = new mapkit.Map(\"map\", { region });","","    const coffeeSearch = new mapkit.Search({","        region,","        regionPriority: mapkit.Search.RegionPriority.Required,","        pointOfInterestFilter: mapkit.PointOfInterestFilter.including([","            mapkit.PointOfInterestCategory.Cafe","        ])","    });","    coffeeSearch.search(\"coffee\", (error, results) => {","        for (const place of results.places) {","            const marker = new mapkit.PlaceAnnotation(place);","            map.addAnnotation(marker);","        }","    });","};"]}],"title":"JavaScript"},{"content":[{"syntax":"swift","code":["\/\/ Finding coffee in Cupertino","","struct CoffeeMap: View {","    @State private var position: MapCameraPosition = .automatic","    @State private var coffeeShops: [MKMapItem] = []","","    var body: some View {","        Map(position: $position) {","            ForEach(coffeeShops, id: \\.self) { café in","                Marker(item: cafe)","            }","        }","        .task {","            guard let cupertino = await findCity() else {","                return","            }","            coffeeShops = await findCoffee(in: cupertino)","        }","    }","","    private func findCity() async -> MKMapItem? {","        let request = MKLocalSearch.Request()","        request.naturalLanguageQuery = \"cupertino\"","","        request.addressFilter = MKAddressFilter(","            including: .locality","        )","","        let search = MKLocalSearch(request: request)","        let response = try? await search.start()","        return response?.mapItems.first","    }","","    private func findCoffee(in city: MKMapItem ) async -> [MKMapItem] {","        let request = MKLocalSearch.Request()","        request.naturalLanguageQuery = \"coffee\"","        let downtown = MKCoordinateRegion(","            center: city.placemark.coordinate,","            span: .init(","                latitudeDelta: 0.01,","                longitudeDelta: 0.01","            )","        )","        request.region = downtown","        request.regionPriority = .required","        let search = MKLocalSearch(request: request)","        let response = try? await search.start()","        return response?.mapItems ?? []","    }","}"],"type":"codeListing"}],"title":"Swift"}]},{"type":"heading","anchor":"Written-By","level":2,"text":"Written By"},{"type":"row","columns":[{"content":[{"inlineContent":[{"type":"image","identifier":"dl-alexandre"}],"type":"paragraph"}],"size":1},{"content":[{"level":3,"text":"Dalton Alexandre","anchor":"Dalton-Alexandre","type":"heading"},{"inlineContent":[{"isActive":true,"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/dl-alexandre","overridingTitle":"Contributed Notes","overridingTitleInlineContent":[{"type":"text","text":"Contributed Notes"}],"type":"reference"},{"text":" ","type":"text"},{"text":"|","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"https:\/\/github.com\/dl-alexandre","type":"reference"},{"text":" ","type":"text"},{"text":"|","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"https:\/\/daltonalexandre.carrd.co","type":"reference"},{"text":" ","type":"text"},{"text":"|","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"https:\/\/x.com\/d_creatio","type":"reference"}],"type":"paragraph"}],"size":4}],"numberOfColumns":5},{"inlineContent":[{"type":"text","text":"Missing anything? Corrections? "},{"isActive":true,"identifier":"https:\/\/wwdcnotes.com\/documentation\/wwdcnotes\/contributing","type":"reference"}],"type":"paragraph"},{"type":"heading","anchor":"Related-Sessions","level":2,"text":"Related Sessions"},{"type":"links","style":"list","items":["doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC23-10043-Meet