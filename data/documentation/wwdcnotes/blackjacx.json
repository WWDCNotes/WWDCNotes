t":[{"text":"Designer turned developer.","type":"text"}]},"doc://WWDCNotes/documentation/WWDCNotes/WWDC19-805-Building-Great-Shortcuts":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC19-805-Building-Great-Shortcuts","kind":"article","title":"Building Great Shortcuts","role":"sampleCode","type":"topic","url":"\/documentation\/wwdcnotes\/wwdc19-805-building-great-shortcuts","abstract":[{"text":"Shortcuts enable people to quickly and easily accomplish actions or get things done hands-free using Siri and the Shortcuts app. Join us for a tour of where shortcuts can appear, how you can customize the experience, and how your app‚Äôs shortcuts can be used with variables and actions from other apps.","type":"text"}]},"doc://WWDCNotes/documentation/WWDCNotes/WWDC19-507-HLS-Authoring-for-AirPlay-2-Video":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/WWDC19-507-HLS-Authoring-for-AirPlay-2-Video","kind":"article","title":"HLS Authoring for AirPlay 2 Video","role":"sampleCode","type":"topic","url":"\/documentation\/wwdcnotes\/wwdc19-507-hls-authoring-for-airplay-2-video","abstract":[{"type":"text","text":"AirPlay 2 Video lets you share video from Apple devices to popular smart TVs. Learn about the special considerations for seamless delivery of high quality video to these TVs, and how to utilize the validation tools to ensure your content is ready for primetime."}]},"doc://WWDCNotes/documentation/WWDCNotes/NinjaLikesCheez":{"identifier":"doc:\/\/WWDCNotes\/documentation\/WWDCNotes\/NinjaLikesCheez","type":"topic","abstract":[{"type":"text","text":"No Bio on GitHub"}],"title":"NinjaLikesCheez (3 notes)","url":"\/documentation\/wwdcnotes\/ninjalikescheez","kind":"article","role":"sampleCode"},"doc://WWDCNotes/documentation/WWDCNotes/SuzGupta":{"title":"Susannah Skyer Gupta (1 note)","abstract":[{"type":"text","text":"Experienced product\/support\/content manager, newbie dev. Passionate about UX, projects with heart and meaning, and the joy of learning